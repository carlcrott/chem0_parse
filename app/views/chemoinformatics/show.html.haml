#header_container
  #title_container.inline
    %h1{:id => "title"} chem0_view
    =link_to("https://bitbucket.org/wwmm/oscar4/wiki/Home", :target => "_blank") do
      %h6
        = @chemoinformatics.count 
        indexes parsed by OSCAR-4

  = link_to(chemoinformatics_path, :id => "chemo_index_link") do
    #nav_button.button.inline browse    

/ %p#notice= notice

%table{ :id => "display_table" }
  %tr
    %th.input_process.top-left-5
      %b Input process:
    %th.top-right-5
      %b Output process:
  %tr.odd
    %td
      = @chemoinformatic.input_process
    %td
      - @chemoinformatic.output_process.split("**!**").each do |line|
        = line
        %br


  %tr
    %th.bottom-left-5
    %th.bottom-right-5
  
%p

  
/ = link_to 'Edit', edit_chemoinformatic_path(@chemoinformatic)

/ #{link_to 'Back', chemoinformatics_path}
